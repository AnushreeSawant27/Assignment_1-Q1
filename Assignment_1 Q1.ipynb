   "source": [
    "# Write a Python program to get the Fibonacci series between 0 to 50"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 3,
   "id": "6cc7af45",
   "metadata": {},
   "outputs": [
    {
     "name": "stdout",
     "output_type": "stream",
     "text": [
      "1 1 2 3 5 8 13 21 34 "
     ]
    }
   ],
   "source": [
    "n=50\n",
    "n1, n2 = 0, 1\n",
    "print(n2, end=\" \")\n",
    "for i in range(0, n):\n",
    "    n3 = n1 + n2\n",
    "    if n3 <= n:\n",
    "        print(n3, end=\" \")\n",
    "        n1 = n2\n",
    "        n2 = n3\n",
    "    "
